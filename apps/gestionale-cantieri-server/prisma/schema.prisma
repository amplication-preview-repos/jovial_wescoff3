datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Cantieri {
  id          String   @id @default(cuid())
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  nome        String?
  descrizione String?
  immagine    Json?
  documenti   Json?
}

model Lavorazioni {
  id          String     @id @default(cuid())
  createdAt   DateTime   @default(now())
  updatedAt   DateTime   @updatedAt
  descrizione String?
  dataInizio  DateTime?
  dataFine    DateTime?
  nome        String?
  ente        String?
  operatore   Operatore? @relation(fields: [operatoreId], references: [id])
  operatoreId String?
}

model Operatore {
  id               String        @id @default(cuid())
  createdAt        DateTime      @default(now())
  updatedAt        DateTime      @updatedAt
  nome             String?
  cognome          String?
  email            String?
  password         String?
  lavorazionis     Lavorazioni[]
  permessiSpeciali Boolean?
}

model Enti {
  id          String   @id @default(cuid())
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  tipo        String?
  nome        String?
  descrizione String?
  cantiere    String?
}

